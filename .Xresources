! URxvt settings
URxvt.borderless:       true
URxvt.externalBorder:   0
URxvt.internalBorder:   20
URxvt.depth:            32
URxvt.loginShell:       true
URxvt.scrollBar:        false
URxvt.cursorUnderline:  true

URxvt.transparent: true
URxvt*shading: 20

! Fonts
URxvt.font:         xft:DejaVuSansMono Nerd Font Mono:size=9
URxvt.letterSpace:  -1

! XFT

Xft.antialias:  true
Xft.autohint:   false
Xft.dpi:        96
Xft.hinting:    true
Xft.hitstyle:   hintslight
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb

! special
*.background:   #111111
*.foreground:   #9ca1b8
*.cursorColor:  #9ca1b8

! black
*.color0:       #484e5b
*.color8:       #5b6373

! red
*.color1:       #e75e87
*.color9:       #e5a9bb

! green
*.color2:       #83d1a2
*.color10:      #b6dac4

! yellow
*.color3:       #dc8b73
*.color11:      #e9c5bb

! blue
*.color4:       #639ce8
*.color12:      #a1c1eb

! magenta
*.color5:       #b972e9
*.color13:      #d3a6f1

! cyan
*.color6:       #62b3e4
*.color14:      #acd1e8

! white
*.color7:       #707880
*.color15:      #c5c8c6
